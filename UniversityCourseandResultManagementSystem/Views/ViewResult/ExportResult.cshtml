@using System.Data.SqlClient
@using System.Security.Cryptography.X509Certificates
@using System.Web.UI.WebControls
@using UniversityCourseandResultManagementSystem.BLL
@using UniversityCourseandResultManagementSystem.Models

@{
    Layout = null;

    List<StudentEnroll> students = ViewBag.StudentList;
    List<ResultForVIew> resultForVIews = ViewBag.ViewResults;
    string message = ViewBag.Message;

}
<!DOCTYPE html>
<html>
<head>
    <title>Export Student Result</title>
    <link rel="stylesheet" href="~/bootstrap/css/bootstrap.min.css">

</head>
<body>

    <form method="POST" id="SaveResultForm">

        <div class="form-group col-md-8 col-md-offset-2">
            <h1 style="text-align:center">Student Result</h1>
            @*<div>
                <label>Name</label>
                <input type="text" name="Name" id="Name" value="<View>" readonly="readonly" class="form-control" />
            </div>
            <div>
                <label>Email</label>
                <input type="text" name="Email" id="Email" value="<View>" readonly="readonly" class="form-control" />
            </div>
            <div>
                <label>Department</label>
                <input type="text" name="Department" id="Department" value="<View>" readonly="readonly" class="form-control" />
            </div>*@
            <table class="table tab-content table-responsive table-bordered">
                <tr>
                    <th>Course Code</th>
                    <th>Name</th>
                    <th>Grade</th>
                </tr>
                @if (resultForVIews != null)
                {
                    foreach (ResultForVIew resultForVIew in resultForVIews)
                    {
                        <tr>
                            <td>@resultForVIew.CourseCode</td>
                            <td>@resultForVIew.CourseName</td>
                            <td>@resultForVIew.Grade</td>
                        </tr>
                    }
                }
            </table>
        </div>




    </form>

    <script src="~/Scripts/jquery-3.1.1.min.js"></script>

    <!-- Bootstrap 3.3.6 -->
    <script src="~/bootstrap/js/bootstrap.min.js"></script>
</body>
</html>